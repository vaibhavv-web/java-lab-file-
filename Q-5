interface A { void showA(); } 

interface B { void showB(); } 

  

class Demo implements A, B { 

    public void showA() { System.out.println("From A"); } 

    public void showB() { System.out.println("From B"); } 

  

    public static void main(String[] args) { 

        Demo D = new Demo(); 

        D.showA(); 

        D.showB(); 

    } 

} 

 
